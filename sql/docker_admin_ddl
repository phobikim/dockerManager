-- DOCKER.docker_admin definition

CREATE TABLE `docker_admin` (
  `ID` varchar(100) NOT NULL,
  `NAME` varchar(100) NOT NULL COMMENT '사용자명',
  `PASSWD` varchar(100) DEFAULT NULL,
  `STATE_CODE` varchar(100) DEFAULT NULL,
  `CREATE_DATE` varchar(100) DEFAULT NULL COMMENT '생성일자'
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4;